{
    "name": "symfony-cmf/block-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Block Bundle",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/BlockBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lrkdev/symfony-cmf-core-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:lrkdev/symfony-cmf-testing.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:lrkdev/symfony-cmf-menu-bundle.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/framework-bundle": "^6.3",
        "doctrine/phpcr-bundle": "^2.4",
        "doctrine/phpcr-odm": "^1.7",
        "sonata-project/block-bundle": "^5.0",
        "symfony-cmf/core-bundle": "dev-master"
    },
    "require-dev": {
        "symfony-cmf/testing": "dev-master",
        "symfony-cmf/menu-bundle": "dev-master",
        "symfony/phpunit-bridge": "^6.3",
        "sonata-project/cache-bundle": "^3.4",

        "twig/twig": "^3.0",
        "symfony/security-bundle": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/asset": "^6.3",
        "symfony/templating": "^6.3",
        "jackalope/jackalope-jackrabbit": "1.*"
    },
    "suggest": {
        "sonata-project/cache-bundle": "To add caching support for block loading",
        "eko/feedbundle": "When using the RssBlock"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "conflict": {
        "sonata/cache": "<1.1.1"
    }
}
